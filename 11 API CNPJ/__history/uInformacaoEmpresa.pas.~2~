unit uInformacaoEmpresa;

interface

type
  TInformacaoEmpresa = class
  private
    FRazao: String;
    FInscricaoEstadual: String;
    FSituacao: String;
    FEmail: String;
    FEnderecoRua: String;
    FEnderecoNumero: String;
    FEnderecoMunicipio: String;
    FEnderecoBairro: String;
    FEnderecoCep: String;
    FEnderecoEstado: String;
  published
    property Razao: String read FRazao write FRazao;
    property InscricaoEstadual: String read FInscricaoEstadual write FInscricaoEstadual;
    property Situacao: String read FSituacao write FSituacao;
    property Email: String read FEmail write FEmail;
    property EnderecoRua: String read FEnderecoRua write FEnderecoRua;
    property EnderecoNumero: String read FEnderecoNumero write FEnderecoNumero;
    property EnderecoMunicipio: String read FEnderecoMunicipio write FEnderecoMunicipio;
    property EnderecoBairro: String read FEnderecoBairro write FEnderecoBairro;
    property EnderecoCep: String read FEnderecoCep write FEnderecoCep;
    property EnderecoEstado: String read FEnderecoEstado write FEnderecoEstado;

  end;

implementation

end.
